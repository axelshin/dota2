html.mobile.fl-popover-opened
html.mobile.fl-popover-opened > body
    -webkit-overflow-scrolling touch !important
    overflow auto !important
    height 100% !important

.fl-popover

    &__container
        position fixed
        top 0
        left 0
        z-index 99999
        width 100%
        direction ltr

    &__dim
        display none
        position fixed
        top 0
        right 0
        bottom 0
        left 0
        background-color #000
        z-index 0
        opacity 0

    &__wrap
        display none
        position relative
        box-sizing border-box
        max-width 100%
        flex 0 0 auto
        margin auto

    &__close
        z-index 2
        position absolute
        top -15px
        right -15px
        width 30px
        height 30px
        cursor pointer
        background-image url('data:image/png;base64,')
        background-repeat no-repeat
        background-position 0 0

    &__body
        max-width 100%
        box-sizing border-box
        position relative
        display inline-block
        padding 20px 40px 35px
        vertical-align middle
        border 1px solid #b7b7b7
        background #fff
        font-family Gotham, -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Helvetica Neue, Arial, sans-serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol, Noto Color Emoji

        &:before
        &:after
            content ""
            display table

        &:after
            clear both

    &__loader 
        display none
        background url("data:image/gif;base64,") no-repeat center rgba(255,255,255,.8)
        position absolute
        top 0
        right 0
        bottom 0
        left 0
        margin 5px
        z-index 1
        cursor pointer

    &__scrolling
        position fixed
        top 0
        right 0
        bottom 0
        left 0
        height 100%
        width 100%
        overflow auto
        z-index 1
        display -webkit-box
        display -moz-box
        display -ms-flexbox
        display -webkit-flex
        display flex
        align-items flex-start
        -ms-flex-align start
        padding 20px
        box-sizing border-box

        &.disable
            overflow hidden
            z-index -1

.circle
    position relative
    z-index 0
    padding (24/16em) (56/16em) (50/16em)
    width (300/16em)
    height (300/16em)
    margin-left (-10/16em)

    +md-block()
        padding (43/16em) (104/16em) (104/16em)
        width (542/16em)
        height (542/16em)
    
    &:before
        position absolute
        z-index -1
        content ''
        display block        
        background-color #fff
        border-radius 50%
        top (37/16em)
        bottom (37/16em)
        left (37/16em)
        right (37/16em)

        +md-block()
            top (67/16em)
            bottom (67/16em)
            left (67/16em)
            right (67/16em)

    &__link
        position absolute
        left 0
        right 0
        top 0
        bottom 0
        display block
        z-index 2
        outline none
        text-indent -100000px
        
    &__arr
        position absolute
        z-index -1
        left 0
        top 0
        width (270/16em)
        height (143/16em)

        svg
            width (270/16em)
            height (143/16em)
            vertical-align top

            path
                fill #E0004D

        +md-block()
            width (490/16em)
            height (258/16em)

            svg
                width (490/16em)
                height (258/16em)

        &--reverse
            left auto
            top auto
            right 0
            bottom 0
            transform rotate(180deg)

    &__gift
        width (53/16em)
        height (50/16em)
        margin-left auto
        margin-right auto
        margin-bottom (22/16em)

        img
            display block
            width 100%

        +md-block()
            width (97/16em)
            height (90/16em)
            margin-bottom (40/16em)
    
    &__title
        color $primary-black
        font-weight 700
        text-align center
        font-size (10/16em)
        margin-bottom (6/10em)

        +md-block()
            font-size 1em
            margin-bottom (14/16em)
    
    &__descr
        display flex
        align-items center
        justify-content center
        line-height 1
        margin-bottom (9/16em)

        +md-block()
            margin-bottom 1em
    
    &__prenum
    &__num
    &__unit
    &__reg
        color $popup2-red
        font-weight 900
        text-transform uppercase
        line-height 1
    
    &__prenum
        font-size 1em
        +md-block()
            font-size (29/16em)
    
    &__num
        font-size (66/16em)

        +md-block()
            font-size (120/16em)

    &__unit
        font-size (26/16em)
        position relative
        top (-3/26em)

        +md-block()
            font-size (46/16em)            
            top (-4/46em)
    
    &__reg
        font-size (10/16em)
        text-transform uppercase
        position relative
        left (2/10em)
        top (3/10em)

        +md-block()
            font-size (17/16em)
            line-height (20/17em)
            position relative
            top (3/17em)
            left (5/17em)

    &__dubble
        display flex
        flex-direction column
        justify-content center

    &__note
        color $primary-black
        text-align center
        font-size (10/16em)
        line-height (12/10em)
        padding-left (8/10em)
        padding-right (8/10em)

        +md-block()
            font-size 1em
            line-height (19/16em)
            padding-left (30/19em)
            padding-right (30/19em)

    &__animation

        animation rotar 10s linear infinite

        &--main
            position absolute
            left 0
            top 0
            right 0
            bottom 0
            z-index -1

    &--hidden
        display none


.circle--btnn
    position fixed
    z-index 11
    text-align center
    text-decoration none
    outline none
    right 20px
    bottom 20px
    width 63px
    height 63px
    padding 15px 10px 0

    &:hover
        text-decoration none
        outline none

    &:before
        top 6px
        right 6px
        left 6px
        bottom 6px

    .circle__arr
    .circle__arr svg
        width 57px
        height 30px
    
    .circle__num
        font-size 23px
        display block
    
    .circle__unit
        font-size 9px
        display block

    +md-block()
        right 20px
        bottom 20px
        width 100px
        height 100px
        padding 24px 20px 0
        

        &:before
            top 12px
            right 12px
            left 12px
            bottom 12px

        .circle__arr
        .circle__arr svg
            width 90px
            height 48px
        
        .circle__num
            font-size 35px
            margin-bottom 2px
        
        .circle__unit
            font-size 14px
    
    +lg-block()
        right 20px
        bottom 10px

.fl-popup

    // font-size calc( 100vw/20 )

    // +md-block()
    //     font-size calc( 100vw/48 )
    
    +xxl2-block()
        font-size calc( 100vw/120 )

    .fl-popover__body
        background none
        border none
        padding 0
    
    .fl-popover__close

        background-image url(../img/content/popup2/popup-close.svg)
        background-size 100% 100%
        width (15/16em)
        height (15/16em)
        right 0
        top (-7/16em)

        +md-block()
            width (22/16em)
            height (22/16em)
            right (-22/16em)
            top (-8/16em)

        +lg-block()
            top (-22/16em)

@keyframes rotar{
    to{
        transform: rotate(-1turn);
    }
}


